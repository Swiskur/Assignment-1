for i in range(5):
    print(i*i)

    #This code shows the first 5 numbers 0-4 in order multiplied by itself or squared.
#b
    
for d in [3,1,4,1,5]:
    print(d, end=" " )

    #This code shows the numbers 3,1,4,1,5 printed in their specific pattern.

#c

for i in range(4):
    print("Hello")

    #This code shows "Hello" printed 4 times as it was asked to do in the range.

#d

for i in range (5):
    print(i, 2**i)

    # This code shows one row as the first 5 numbers 0-4 and the other row starts at 1 and multiplies itslelf by 2 5 times showing.1,2,4,8,16.

